{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": ["tsconfig.json"]
  },
  "plugins": ["@typescript-eslint"],
  "extends": ["airbnb-typescript/base"],
  "rules": {
    "max-len": ["error", { "code": 110 }],
    "no-param-reassign": ["error", { "props": false }]
  }
}