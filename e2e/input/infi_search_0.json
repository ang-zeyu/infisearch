{
  "fields_config": {
    "num_docs_per_store": 200,
    "num_stores_per_dir": 1000,
    "fields": {
      "title": {
        "weight": 2.0,
        "k": 1.2,
        "b": 0.15
      },
      "h1": null,
      "heading": {
        "weight": 1.5,
        "k": 1.2,
        "b": 0.15
      },
      "body": {
        "weight": 1.0,
        "k": 1.2,
        "b": 0.75
      },
      "headingLink": {
        "weight": 0.0,
        "k": 1.2,
        "b": 0.75
      },
      "_relative_fp": {
        "weight": 0.0,
        "k": 1.2,
        "b": 0.75
      },
      "weatherField": {
        "storage": ["enum"],
        "weight": 0.0
      },
      "moodField": {
        "storage": ["enum"]
      }
    }
  },
  "lang_config": {
    "lang": "ascii",
    "options": {
      "stop_words": ["typesetting"],
      "ignore_stop_words": false
    }
  },
  "indexing_config": {
    "num_docs_per_block": 2,
    "pl_limit": 16383,
    "pl_cache_threshold": 0,
    "exclude": [
      "index.html",
      "print.html",
      "infi_search_*",
      "**/secondary_*"
    ],
    "loaders": {
      "HtmlLoader": {
        "exclude_selectors": [
          "script,style,#sidebar,#menu-bar,[data-infisearch-ignore]"
        ],
        "selectors": {
          "title": {
            "field_name": "title"
          },
          "h1": null,
          "h2,h3,h4,h5,h6": null,
          "h1,h2,h3,h4,h5,h6": {
            "attr_map": {
              "id": "headingLink"
            },
            "field_name": "heading"
          },
          "body": {
            "field_name": "body"
          },
          "div[also-index]": {
            "attr_map": {
              "also-index": "_add_files"
            }
          },
          "[data-infisearch-weather]": {
            "attr_map": {
              "data-infisearch-weather": "weatherField"
            }
          },
          "[data-infisearch-mood]": {
            "attr_map": {
              "data-infisearch-mood": "moodField"
            }
          }
        }
      },
      "JsonLoader": {
        "field_map": {
          "body": "body",
          "heading": "heading",
          "title": "title"
        },
        "field_order": [
          "title",
          "heading",
          "body"
        ],
        "type": "JsonLoader"
      },
      "CsvLoader": {
        "header_field_map": {},
        "header_field_order": [],
        "index_field_map": {
          "0": "title",
          "1": "body",
          "2": "_add_files"
        },
        "index_field_order": [
          0,
          1,
          2
        ],
        "parse_options": {
          "comment": null,
          "delimiter": 44,
          "double_quote": true,
          "escape": null,
          "has_headers": true,
          "quote": 34
        },
        "type": "CsvLoader",
        "use_headers": false
      },
      "PdfLoader": {}
    },
    "num_pls_per_dir": 1000,
    "with_positions": true
  }
}