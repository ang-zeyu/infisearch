{
  "fields_config": {
    "field_store_block_size": 100,
    "num_stores_per_dir": 1000,
    "fields": [
      {
        "name": "title",
        "do_store": true,
        "weight": 2.0,
        "k": 1.2,
        "b": 0.15
      },
      {
        "name": "heading",
        "do_store": true,
        "weight": 1.5,
        "k": 1.2,
        "b": 0.25
      },
      {
        "name": "body",
        "do_store": true,
        "weight": 1.0,
        "k": 1.2,
        "b": 0.75
      },
      {
        "name": "headingLink",
        "do_store": true,
        "weight": 0.0,
        "k": 1.2,
        "b": 0.75
      },
      {
        "name": "_relative_fp",
        "do_store": true,
        "weight": 0.0,
        "k": 1.2,
        "b": 0.75
      }
